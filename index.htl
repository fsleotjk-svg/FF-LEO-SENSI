</select><button onclick="generate()">GET SENSI</button>

<div id="result" class="card"></div>
</div><script>
function r(min,max){return Math.floor(Math.random()*(max-min+1))+min}

function phoneBoost(name){
 name=name.toLowerCase()
 let b=0
 if(name.includes('redmi')) b=5
 if(name.includes('samsung')) b=7
 if(name.includes('iphone')) b=10
 if(name.includes('poco')) b=6
 return b
}

function smart(base,hs,aim,lock,boost){
 let v = base + boost
 v += hs>94?5:2
 v += aim>95?4:2
 v += lock>95?4:2
 return Math.min(200, Math.max(0,v))
}

function generate(){
 let phone=document.getElementById('phone').value||"default"
 let hs=+document.getElementById('hs').value
 let recoil=+document.getElementById('recoil').value
 let aim=+document.getElementById('aim').value
 let lock=+document.getElementById('lock').value
 let fire=+document.getElementById('fire').value

 let boost=phoneBoost(phone)

 let general = smart(r(130,185),hs,aim,lock,boost)
 let red = smart(r(100,185),hs,aim,lock,boost)
 let x2 = smart(r(100,185),hs,aim,lock,boost)
 let x4 = smart(r(101,170),hs,aim,lock,boost)
 let awm = smart(r(100,150),hs,aim,lock,boost)
 let look = smart(r(70,150),hs,aim,lock,boost)

 let stable = (100-recoil) + hs/2 + aim/2

 document.getElementById('result').innerHTML=`
 <b>ðŸ“± Phone:</b> ${phone}<br>
 <b>ðŸŽ¯ General:</b> ${general}<br>
 <b>ðŸŽ¯ RedDot:</b> ${red}<br>
 <b>ðŸŽ¯ 2x Scope:</b> ${x2}<br>
 <b>ðŸŽ¯ 4x Scope:</b> ${x4}<br>
 <b>ðŸŽ¯ AWM:</b> ${awm}<br>
 <b>ðŸŽ¯ Free Look:</b> ${look}<br>
 <b>ðŸ”¥ Fire Button:</b> ${fire}<br>
 <b>â™¾ Stable:</b> ${stable.toFixed(1)}%<br>
 <b>âš™ Recoil:</b> ${recoil}%<br>
 <b>ðŸŽ¯ Headshot Rate:</b> ${hs}%<br>
 <b>ðŸ”’ Aim Lock:</b> ${lock}%<br>
 `
}
</script></body>
